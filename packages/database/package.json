{
  "dependencies": {
    "@prisma/client": "6.13.0"
  },
  "devDependencies": {
    "@yarnpkg/pnpify": "^4.1.4",
    "prisma": "6.13.0"
  },
  "main": "src/index.js",
  "name": "@tsm-example/database",
  "packageManager": "yarn@4.9.1",
  "scripts": {
    "database": "./scripts/database.sh",
